{
  {
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {"@/*": ["./src/*"]}
    "resolveJsonModule",: true,
    "isolatedModules": true,
    "jsx": "preserve",:
    "module": "esnext",:
    "esModuleInterop",: true,
    "allowSyntheticDefaultImports",: true,
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
  }
}
